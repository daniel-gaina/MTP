spec! LIST =
 rigid sort List .
end

spec! MyLogic{X :: TRIV} =
  pr LIST .

  rigid sort Elt .
  nominal sort Sort2 .

  var  E : Elt .
  vars L L' : List .

  op empty : -> List [ctor] .
  op _._ : Elt List -> List [ctor] .

  op head : List -> Elt .
  eq head(E . L) = E .
  ceq head(E . L) = E
   if L = empty /\
      L = empty .

end